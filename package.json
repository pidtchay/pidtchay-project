{
  "name": "pidtchay-project",
  "version": "0.2.1",
  "homepage": "https://pidtchay.github.io/",
  "description": "Personal project on the react framework",
  "main": "index.js",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack.js --config webpack/webpack.prod.js",
    "codestyle": "npm run prettier && npm run tslint",
    "precommit": "npm run codestyle && npm run preversion",
    "prettier": "npx prettier --write src/*",
    "preversion:minor": "git-branch-is -r \"^feature/\" && npm version minor",
    "preversion:patch": "git-branch-is -r \"^(bugfix|hotfix)/\" && npm version patch",
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --host localhost --port 5454 --open --hot --config webpack/webpack.dev.js",
    "test": "jest --colors",
    "test:updateShapshots": "jest --updateSnapshot --colors",
    "tslint": "tslint -p tslint -p tsconfig.json",
    "tsc": "node max-old-space-size=4096 node_modules/typescript/bin/tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run codestyle"
    }
  },
  "keywords": [
    "react",
    "redux",
    "typescript"
  ],
  "author": "pidtchay",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.2.5",
    "git-branch-is": "^3.1.0",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "@types/jsdom-global": "^3.0.2",
    "@types/node": "^14.0.12",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
